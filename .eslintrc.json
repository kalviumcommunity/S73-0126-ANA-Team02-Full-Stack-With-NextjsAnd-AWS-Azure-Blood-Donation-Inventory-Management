{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "no-console": "warn",
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "semi": true,
        "tabWidth": 2,
        "trailingComma": "es5"
      }
    ]
  }
}
